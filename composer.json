{
    "name": "gassam_kevin/laravel-wenguii",
    "description": "Laravel package for WenGuii API integration",
    "keywords": [
        "laravel",
        "wenguii",
        "billing",
        "php",
        "github",
        "Open Source",
        "action",
        "service",
        "traits",
        "generator",
        "command",
        "laravel 7",
        "laravel 8",
        "laravel 9",
        "laravel 10",
        "laravel 11"
    ],
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Wenguii\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Gassam Kevin",
            "email": "contact@kg-code.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^7.1|^8.0",
        "guzzlehttp/guzzle": "^6.3",
        "illuminate/support": "^5.6|^5.7|^5.8|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wenguii\\WenguiiServiceProvider"
            ]
        }
    }
}
